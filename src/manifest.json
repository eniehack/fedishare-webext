{
    "manifest_version": 2,
    "name": "fideshare",
    "version": "0.1.0",
    "options_ui": {
        "page": "options_ui/index.html"
    },
    "permissions": ["storage"],
    "applications": {
        "gecko": {
            "id": "suruttofedishare@eniehack"
        }
    },
    "web_accessible_resources": [
        "provider.js"
    ],
    "content_script": [
        {
            "matched": "<all_urls>",
            "js": ["content_script/insert_provider.js"]
        }
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';"
}